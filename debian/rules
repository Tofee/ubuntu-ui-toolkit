#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1
export DPKG_GENSYMBOLS_CHECK_LEVEL=4

%:
	dh $@ --fail-missing

override_dh_install:
	mkdir -p debian/tmp/`qmake -query QT_INSTALL_DOCS`/qch 
	cp -a documentation/html/ubuntuuserinterfacetoolkit.qch debian/tmp/usr/share/qt5/doc/qch/
	mkdir -p debian/tmp/usr/share/ubuntu-ui-toolkit/doc
	cp -r documentation/html debian/tmp/usr/share/ubuntu-ui-toolkit/doc
	rm -rf debian/tmp/usr/share/locale
	dh_install --fail-missing

